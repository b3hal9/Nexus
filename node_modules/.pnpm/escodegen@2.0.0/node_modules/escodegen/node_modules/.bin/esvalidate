#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/esprima@4.0.1/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules:/home/bishal/MySpace/Final/finalpms/node_modules:/home/bishal/MySpace/Final/node_modules:/home/bishal/MySpace/node_modules:/home/bishal/node_modules:/home/node_modules:/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/escodegen@2.0.0/node_modules/escodegen/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/escodegen@2.0.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/esprima@4.0.1/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules:/home/bishal/MySpace/Final/finalpms/node_modules:/home/bishal/MySpace/Final/node_modules:/home/bishal/MySpace/node_modules:/home/bishal/node_modules:/home/node_modules:/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/escodegen@2.0.0/node_modules/escodegen/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/escodegen@2.0.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../esprima@4.0.1/node_modules/esprima/bin/esvalidate.js" "$@"
else
  exec node  "$basedir/../../../../../esprima@4.0.1/node_modules/esprima/bin/esvalidate.js" "$@"
fi
