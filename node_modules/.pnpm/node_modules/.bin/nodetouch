#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/touch@3.1.0/node_modules/touch/bin/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/touch@3.1.0/node_modules/touch/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/touch@3.1.0/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules:/home/bishal/MySpace/Final/finalpms/node_modules:/home/bishal/MySpace/Final/node_modules:/home/bishal/MySpace/node_modules:/home/bishal/node_modules:/home/node_modules:/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/node_modules/touch/bin/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/node_modules/touch/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/touch@3.1.0/node_modules/touch/bin/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/touch@3.1.0/node_modules/touch/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/touch@3.1.0/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules:/home/bishal/MySpace/Final/finalpms/node_modules:/home/bishal/MySpace/Final/node_modules:/home/bishal/MySpace/node_modules:/home/bishal/node_modules:/home/node_modules:/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/node_modules/touch/bin/node_modules:/home/bishal/MySpace/Final/finalpms/server/node_modules/.pnpm/node_modules/touch/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../touch/bin/nodetouch.js" "$@"
else
  exec node  "$basedir/../touch/bin/nodetouch.js" "$@"
fi
